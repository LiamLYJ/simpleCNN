        {
            "name": "Python: mainpy",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/torch_conv.py",
            "console": "integratedTerminal"
        },
        
        {
            "name": "(lldb) main Launch",
            "type": "cppdbg",
            "request": "launch",
