            "MIMode": "lldb",
            "miDebuggerPath": "/Applications/Xcode.app/Contents/Developer/usr/bin/lldb-mi",
            "preLaunchTask": "main_build"
        },
        {
            "name": "(lldb)play_ground",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/play_ground",
